{
  "name": "laravel-nuxt",
  "private": true,
  "type": "module",
  "scripts": {
    "serve": "cross-env PORT=3000 HOST=127.0.0.1 node .output/server/index.mjs",
    "build": "nuxi cleanup && nuxi build",
    "cleanup": "nuxi cleanup",
    "dev": "nuxt dev --port=3000 --host=127.0.0.1",
